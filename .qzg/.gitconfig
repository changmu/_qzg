[user]
	name = windqiu
	email = windqiu@futunn.com
[color]
    ui = true
[alias]
    st = status
    cm = commit
    unstage = reset HEAD
    co = checkout
    br = branch

    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[core]
	autocrlf = false
	editor = vim
	filemode = true
[commit]
    template = ~/.tplCommitMsg
[safe]
	directory = *
[url "git@gitlab.futunn.com:"]
	insteadOf = http://gitlab.futunn.com/
	insteadOf = https://gitlab.futunn.com/
[url "git@github.com:"]
	insteadOf = https://github.com/
